@page
@{
ViewData["Title"] = "Manage Tables";
Layout = "~/Pages/Shared/_AdminLayout.cshtml";
}


<div class="login-background nopadding splash nomargins">
    <div class="container">
        <div class="row">
            <div class="col-sm-12">
                <div class="login-box pagination-lg">
                    <h2 class="text-center">Manage Tables</h2>
                    <div class="form-group center-in-div">
                        <form method="get">
                            <button type="submit" class="btn btn-primary" asp-page="/ValueOfHours/Index">Value of
                                Hours</button>
                        </form>
                    </div>
                    <div class="form-group center-in-div">
                        <form method="get">
                            <button type="submit" class="btn btn-primary"
                                asp-page="/Initiatives/Index">Initiatives</button>
                        </form>
                    </div>
                    <div class="form-group center-in-div">
                        <form method="get">
                            <button type="submit" class="btn btn-primary" asp-page="/VolunteerTypes/Index">Volunteer
                                Types</button>
                        </form>
                    </div>
                    <div class="form-group center-in-div">
                        <form method="get">
                            <button type="submit" class="btn btn-primary" asp-page="/Races/Index">Races</button>
                        </form>
                    </div>
                    <div class="form-group center-in-div">
                        <form method="get">
                            <button type="submit" class="btn btn-primary" asp-page="/EducationLevels/Index">Education
                                Levels</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>